  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/node": "^24.1.0",
    "@types/react": "~19.0.10",
    "@types/react-native": "^0.72.8",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0",
    "typescript": "~5.8.3"
  },
